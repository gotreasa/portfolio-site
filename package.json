  "main": "index.js",
  "scripts": {
    "lint:fix": "eslint --fix",
    "lint:verify": "eslint .",
    "lint:verifyfile": "eslint",
    "precommit": "lint-staged --relative --verbose",
    "prepare": "husky install",
    "prettier:fix": "prettier --write",
    "prettier:verify": "prettier --check \"*.{js,css,scss,md,html,yml,yaml,json}\"",
    "prettier:verifyfile": "prettier --check",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix"
    ],
    "*.{js,css,scss,md,html,yml,yaml,json}": [
      "npm run prettier:fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@extractus/feed-extractor": "^7.0.0",
    "moment": "^2.29.4",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.6",
    "commitlint": "^17.6.6",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jshint": "^2.13.6",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "sort-package-json": "^2.5.1"
  }
}
