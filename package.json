  "scripts": {
    "build": "hugo --gc --minify --templateMetrics --templateMetricsHints --buildDrafts --buildExpired --buildFuture --forceSyncStatic",
    "lint:fix": "eslint --fix",
    "lint:verify": "eslint .",
    "lint:verifyfile": "eslint",
    "precommit": "lint-staged --relative --verbose",
    "prepare": "husky",
    "prettier:fix": "prettier --write",
    "prettier:verify": "prettier --check \"*.{js,css,scss,md,html,yml,yaml,json}\"",
    "prettier:verifyfile": "prettier --check",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-modules": "node ./scripts/clearModules.js && hugo mod clean --all && hugo mod get -u ./... && hugo mod tidy && npm i",
    "update-posts": "node src/posts/transform-posts.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix"
    ],
    "*.{js,css,scss,md,html,yml,yaml,json}": [
      "npm run prettier:fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
